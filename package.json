{
  "name": "library-compare",
  "version": "1.0.0",
  "description": "Compare game libraries across platforms",
  "main": "build/main/main.js",
  "homepage": "./",
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "start": "electron-vite dev --watch",
    "build": "electron-vite build",
    "dist": "pnpm build && electron-builder",
    "dist:dir": "pnpm build && electron-builder --dir",
    "dist:linux": "pnpm build && electron-builder --linux",
    "dist:mac": "pnpm build && electron-builder --mac",
    "dist:win": "pnpm build && electron-builder --win",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rimraf build release node_modules/.vite"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^3.0.1",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "electron-store": "^8.1.0",
    "lucide-react": "^0.263.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.2",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "autoprefixer": "^10.4.14",
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.8.3"
  }
}
